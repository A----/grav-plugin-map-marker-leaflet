{% for point in points %}
    L.marker([{{ point[0] }},{{ point[1] }}], {
        icon: L.mapquest.icons.{{type}}(
        {
          primaryColor: {{ primaryColor }},
          secondaryColor: {{ secondaryColor }},
          shadow: {% if shadow %}true{% else %}false{% endif %},
          size: {{ size }},
          symbol: {{ symbol }}{% if enum and type is same as('flag') %}+{{ loop.index0 }}{% endif %}
          }){% if titles %}, title: "{{ titles[loop.index0] }}"{% endif %}
          {% if draggable %}, draggable: true{% endif %}
        }).addTo(map);
{% endfor %}
